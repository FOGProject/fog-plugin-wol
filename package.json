{
  "name": "fog-plugin-wol",
  "private": true,
  "version": "0.0.1",
  "description": "A wake-on-lan plugin for fog 2.0",
  "dependencies": {
    "node-wol": "^0.1.1",
    "sails-util-mvcsloader": "^0.4.0"
  },
  "fog": {
    "isPlugin": true,
    "hooks": {
      "host.deploy": "sails.helpers.wol.wakeHost",
      "host.capture": "sails.helpers.wol.wakeHost"
    }
  },
  "main": "index.js",
  "sails": {
    "isHook": true
  }
}
